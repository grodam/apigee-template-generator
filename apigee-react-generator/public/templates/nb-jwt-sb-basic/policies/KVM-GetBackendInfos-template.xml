<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<KeyValueMapOperations continueOnError="false" enabled="true" name="KVM-GetBackendInfos" mapIdentifier="{{kvmName}}">
    <DisplayName>KVM-GetBackendInfos</DisplayName>
    <Properties/>
    <ExclusiveCache>false</ExclusiveCache>
    <ExpiryTimeInSecs>300</ExpiryTimeInSecs>
    <Get assignTo="private.backend_id">
        <Key>
            <Parameter>backend_id</Parameter>
        </Key>
    </Get>
    <Get assignTo="private.backend_secret">
        <Key>
            <Parameter>backend_secret</Parameter>
        </Key>
    </Get>
    <Scope>environment</Scope>
</KeyValueMapOperations>
